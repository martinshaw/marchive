{
  "name": "marchive",
  "version": "0.10.0",
  "description": "The Ultimate Information, News and Media Archiver and Aggregator",
  "keywords": [
    "electron",
    "marchive",
    "archiver",
    "downloader",
    "archive"
  ],
  "homepage": "https://github.com/martinshaw/marchive#readme",
  "bugs": {
    "url": "https://github.com/martinshaw/marchive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinshaw/marchive.git"
  },
  "private": true,
  "author": {
    "name": "Martin Shaw",
    "email": "developer@martinshaw.co",
    "url": "https://martinshaw.co"
  },
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "pnpm": ">=8.0.0",
    "npm": ">=10.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "compile": "nx run-many -t compile --parallel",
    "setup": "nx run-many -t setup --parallel && pnpm run compile",
    "cli:dev": "cd packages/cli && pnpm run dev",
    "cli:compile": "cd packages/cli && pnpm run compile",
    "cli:compile:production": "cd packages/cli && pnpm run compile:production",
    "cli:pack:mac": "cd packages/cli && pnpm run pack:macos",
    "cli:pack:linux": "cd packages/cli && pnpm run pack:linux",
    "cli:pack:win": "cd packages/cli && pnpm run pack:win",
    "electron-app:start": "cd packages/electron-app && pnpm run start",
    "electron-app:build": "cd packages/electron-app && pnpm run build",
    "electron-app:package": "cd packages/electron-app && pnpm run package",
    "data-providers:chromium-install": "cd packages/data-providers && pnpm run chromium-install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "nx": "17.3.1"
  },
  "dependencies": {
    "data-providers": "workspace:^"
  }
}