{
  "name": "marchive",
  "version": "0.10.0",
  "description": "The Ultimate Information, News and Media Archiver and Aggregator",
  "keywords": [
    "electron",
    "marchive",
    "archiver",
    "downloader",
    "archive"
  ],
  "homepage": "https://github.com/martinshaw/marchive#readme",
  "bugs": {
    "url": "https://github.com/martinshaw/marchive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinshaw/marchive.git"
  },
  "private": true,
  "author": {
    "name": "Martin Shaw",
    "email": "developer@martinshaw.co",
    "url": "https://martinshaw.co"
  },
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "pnpm": ">=8.0.0",
    "npm": ">=10.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "compile": "nx run-many -t compile --parallel",
    "setup": "nx run-many -t setup --parallel && pnpm run compile",
    "cli:dev": "pnpm run --filter cli dev",
    "cli:compile": "pnpm run --filter cli compile",
    "cli:compile:production": "pnpm run --filter cli compile:production",
    "cli:pack:mac": "pnpm run --filter cli pack:macos",
    "cli:pack:linux": "pnpm run --filter cli pack:linux",
    "cli:pack:win": "pnpm run --filter cli pack:win",
    "electron-app:start": "pnpm run --filter electron-app start",
    "electron-app:build": "pnpm run --filter electron-app build",
    "electron-app:package": "pnpm run --filter electron-app package",
    "data-providers:chromium-install": "pnpm run --filter data-providers chromium-install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "nx": "17.3.1"
  },
  "dependencies": {
    "data-providers": "workspace:^"
  }
}